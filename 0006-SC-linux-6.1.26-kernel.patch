diff -rup linux-6.1.26.orig/kernel/panic.c linux-6.1.26/kernel/panic.c
--- linux-6.1.26.orig/kernel/panic.c	2023-04-26 22:28:44.000000000 +1000
+++ linux-6.1.26/kernel/panic.c	2023-05-03 13:12:08.398051542 +1000
@@ -690,6 +690,24 @@ void warn_slowpath_fmt(const char *file,
 	bool rcu = warn_rcu_enter();
 	struct warn_args args;
 
+#ifdef CONFIG_ARM_64KB_MMU_PAGE_SIZE_SUPPORT
+	static int num_warnings = 0;
+	/*
+	 * We're getting lots of these warnings
+	 * with 64K kernel and they're generally
+	 * all the same (Something relating to bpf).
+	 * Only bother printing the warnings every
+	 * few hundred or so. I know this is awful
+	 * but once you've seen one of these messages
+	 * you've seen them all.
+	 */
+	num_warnings = (num_warnings + 1) & 255;
+	if (num_warnings == 1) {
+		printk ("NOTICE : Suppressing the next 256 warn_slowpath_fmt messages\n");
+	} else {
+		return;
+	}
+#endif
 	pr_warn(CUT_HERE);
 
 	if (!fmt) {
